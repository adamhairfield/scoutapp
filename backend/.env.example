# Server Configuration
PORT=3001
NODE_ENV=development

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:19006,exp://localhost:19000,http://192.168.0.152:19006,https://adamhairfield.github.io,http://localhost:3000,http://172.20.10.3:19006,exp://172.20.10.3:19000

# Rate Limiting
RATE_LIMIT_POINTS=100
RATE_LIMIT_DURATION=60

# Puppeteer Configuration
PUPPETEER_HEADLESS=true
PUPPETEER_ARGS=--no-sandbox,--disable-setuid-sandbox

# Manus AI Configuration
MANUS_AI_API_KEY=your-manus-ai-api-key-here
MANUS_AI_BASE_URL=https://api.manus.ai
MANUS_WEBHOOK_URL=https://your-netlify-site.netlify.app/manus-webhook.html

# Logging
LOG_LEVEL=info
